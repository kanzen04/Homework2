  <time datetime="1910-1-12" itemprop="birthDate">
<a href="/search/name?birth_monthday=1-12&refine=birth_monthday&ref_=nm_ov_bth_monthday" >January 12</a>, 
     
<a href="/search/name?birth_year=1910&ref_=nm_ov_bth_year" >1910</a>    
  </time>
            in
            <a href="/search/name?birth_place=Düsseldorf, North Rhine-Westphalia, Germany&ref_=nm_ov_bth_place" >Düsseldorf, North Rhine-Westphalia, Germany</a>
    </div>

                        
                        
