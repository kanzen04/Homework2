  <time datetime="1959-8-14" itemprop="birthDate">
<a href="/search/name?birth_monthday=8-14&refine=birth_monthday&ref_=nm_ov_bth_monthday" >August 14</a>, 
     
<a href="/search/name?birth_year=1959&ref_=nm_ov_bth_year" >1959</a>    
  </time>
            in
            <a href="/search/name?birth_place=La Jolla, California, USA&ref_=nm_ov_bth_place" >La Jolla, California, USA</a>
    </div>

                        
                        
