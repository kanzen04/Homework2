  <time datetime="1958-10-16" itemprop="birthDate">
<a href="/search/name?birth_monthday=10-16&refine=birth_monthday&ref_=nm_ov_bth_monthday" >October 16</a>, 
     
<a href="/search/name?birth_year=1958&ref_=nm_ov_bth_year" >1958</a>    
  </time>
            in
            <a href="/search/name?birth_place=West Covina, California, USA&ref_=nm_ov_bth_place" >West Covina, California, USA</a>
    </div>

                        
                        
