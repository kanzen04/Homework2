  <time datetime="1921-6-21" itemprop="birthDate">
<a href="/search/name?birth_monthday=6-21&refine=birth_monthday&ref_=nm_ov_bth_monthday" >June 21</a>, 
     
<a href="/search/name?birth_year=1921&ref_=nm_ov_bth_year" >1921</a>    
  </time>
            in
            <a href="/search/name?birth_place=New York City, New York, USA&ref_=nm_ov_bth_place" >New York City, New York, USA</a>
    </div>

